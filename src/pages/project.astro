---
import BaseHead from "../components/BaseHead.astro";
import { SITE_TITLE } from "../config";
import Header from "@components/Header.astro";
import Body from "@components/Body.astro";
import Footer from "@components/Footer.astro";
import ProjectPost from "./project/projectPosts";
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description="HOME" />
  </head>

  <Header />
  <Body>
    <main class="mx-[10%]" aria-label="project page">
      <ProjectPost client:only="react" />
    </main>
    <Footer />
  </Body>

</html>
